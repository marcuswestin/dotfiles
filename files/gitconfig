[color]
  branch = auto
  diff = auto
  status = auto

[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green

[color "diff"]
  meta = blue bold
  frag = magenta bold
  old = red bold
  new = green

[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[user]
	name = Marcus Westin
	email = marcus.westin@gmail.com
[core]
	excludesfile = ~/.gitignore
[alias]
	l = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --
